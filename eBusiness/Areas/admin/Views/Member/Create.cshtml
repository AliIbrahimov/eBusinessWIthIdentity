
@{
    ViewData["Title"] = "Create";
}
@model Member
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name">Name</label>
        <input asp-for="Name" class="form-control" placeholder="Enter name">
    </div>
    <div class="form-group">
        <label asp-for="Position">Position</label>
        <input asp-for="Position" class="form-control" placeholder="Enter position">
    </div>
    <div class="form-group">
        <label asp-for="FormFile">Image</label>
        <input asp-for="FormFile" class="form-control" accept="image/*" type="file">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

